{
  "name": "BaselineResults",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.dell:jordanjones/BaselineResults.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ],
    "cache": {},
    "packageCache": {},
    "fullPaths": true
  },
  "browser": {
    "bootstrap": "./app/bower_components/bootstrap/dist/js/bootstrap.js",
    "c3": "./app/bower_components/c3/c3.js",
    "d3": "./app/bower_components/d3/d3.js",
    "jasny-bootstrap": "./app/bower_components/jasny-bootstrap/dist/js/jasny-bootstrap.js",
    "jquery": "./app/bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "d3": {
      "exports": "d3"
    },
    "c3": {
      "exports": "c3",
      "depends": [
        "d3"
      ]
    }
  },
  "dependencies": {
    "moment": "^2.8.4",
    "react": "^0.12.1",
    "react-router": "^0.11.2",
    "when": "^3.6.3"
  },
  "devDependencies": {
    "browserify": "^6.3.2",
    "browserify-shim": "^3.8.0",
    "del": "^0.1.3",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-bower": "0.0.7",
    "gulp-cache": "^0.2.4",
    "gulp-gh-pages": "^0.4.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.0.0",
    "gulp-jshint": "^1.9.0",
    "gulp-load-plugins": "^0.7.1",
    "gulp-react": "^2.0.0",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-size": "^1.1.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-useref": "^1.0.2",
    "gulp-util": "^3.0.1",
    "gulp-webserver": "^0.8.7",
    "reactify": "^0.17.1",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
